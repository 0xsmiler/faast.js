{
    "name": "faastjs",
    "version": "0.0.2",
    "description": "Serverless without the cruft.",
    "main": "build/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/acchou/faast.js"
    },
    "typings": "build/index.d.ts",
    "scripts": {
        "build": "npm run clean && tsc && npm run doc",
        "watch": "npm run clean && tsc --watch",
        "doc": "api-extractor run --local && api-documenter markdown -i build -o docs/api",
        "aws-set-gcp-key": "node build/src/aws/aws-set-gcp-key.js",
        "test": "ava build/test/*.test.js",
        "test-local": "ava --concurrency=4 -m='!remote*'",
        "test-aws": "ava --concurrency=8 --timeout=2m -m='!remote*' -m='*aws*'",
        "test-google": "ava --concurrency=4 --timeout=4m -m='!remote*' -m='*google*'",
        "test-debug": "DEBUG=faast:provider,faast:info ava --fail-fast --serial",
        "cleanup-cloud": "node build/src/cli.js cleanup aws -x -f && node ./build/src/cli.js cleanup google -x -f",
        "clean": "rm -rf build",
        "tslint": "tslint -p tsconfig.json"
    },
    "author": "Andy Chou",
    "license": "ISC",
    "bin": {
        "faastjs": "./build/src/cli.js"
    },
    "dependencies": {
        "archiver": "^3.0.0",
        "aws-sdk": "^2.403.0",
        "commander": "^2.15.1",
        "debug": "^4.1.1",
        "fs-extra": "^7.0.1",
        "googleapis": "^37.2.0",
        "listr": "^0.14.3",
        "memory-fs": "^0.4.1",
        "ora": "^3.1.0",
        "process-doctor": "^1.0.0",
        "tslib": "^1.9.1",
        "uuid": "^3.2.1",
        "webpack": "^4.29.3",
        "webpack-node-externals": "^1.7.2",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "@microsoft/api-documenter": "^7.0.26",
        "@microsoft/api-extractor": "^7.0.19",
        "@types/archiver": "^2.1.1",
        "@types/aws-lambda": "^8.10.19",
        "@types/debug": "^4.1.1",
        "@types/express": "^4.16.1",
        "@types/listr": "^0.13.0",
        "@types/loader-utils": "^1.1.3",
        "@types/lolex": "0.0.0",
        "@types/memory-fs": "^0.3.0",
        "@types/node": "^11.9.4",
        "@types/ora": "^3.0.0",
        "@types/papaparse": "^4.5.7",
        "@types/tar-stream": "^1.6.0",
        "@types/uuid": "^3.4.3",
        "@types/webpack": "^4.4.24",
        "@types/webpack-node-externals": "^1.6.3",
        "@types/yauzl": "^2.9.1",
        "ava": "^1.2.1",
        "lolex": "^3.1.0",
        "papaparse": "^4.6.3",
        "tslint": "^5.12.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.3.3",
        "webpack-cli": "^3.2.3"
    },
    "ava": {
        "files": [
            "build/test/*.test.js"
        ]
    }
}
