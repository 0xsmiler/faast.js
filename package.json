{
    "name": "cloudify",
    "version": "0.0.1",
    "description": "Serverless without the cruft.",
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "dist": "zip -r dist.zip package.json build",
        "test": "jest"
    },
    "author": "",
    "license": "ISC",
    "engines": {
        "node": ">=6.11.0"
    },
    "dependencies": {
        "archiver": "^2.1.1",
        "axios": "*",
        "googleapis": "*",
        "human-stringify": "*",
        "memory-fs": "^0.4.1",
        "source-map-support": "^0.5.6",
        "tslib": "*",
        "webpack": "^4.8.1",
        "webpack-node-externals": "^1.7.2"
    },
    "devDependencies": {
        "@types/archiver": "^2.1.1",
        "@types/express": "*",
        "@types/jest": "*",
        "@types/memory-fs": "^0.3.0",
        "@types/node": "*",
        "@types/webpack": "^4.1.5",
        "@types/webpack-node-externals": "^1.6.3",
        "jest": "*",
        "ts-jest": "*",
        "typescript": "*",
        "webpack-cli": "^2.1.3"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
        "roots": ["test"]
    }
}
