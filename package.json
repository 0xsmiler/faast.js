{
    "name": "faastjs",
    "version": "0.0.2",
    "description": "Serverless without the cruft.",
    "main": "build/src/faast.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/acchou/faast.js"
    },
    "types": "build/src/faast.d.ts",
    "scripts": {
        "build": "npm run clean && tsc",
        "watch": "npm run clean && tsc --watch",
        "aws-set-gcp-key": "node build/src/aws/aws-set-gcp-key.js",
        "test": "jest build/test/*",
        "test-slow": "jest -w 1 build/test-slow/",
        "test-unit": "jest build/test/unit-*",
        "test-aws": "jest -w 2 build/test/unit-* build/test/aws-*",
        "test-google": "jest -w 2 build/test/unit-* build/test/google-*",
        "test-pipeline": "jest -w 4 build/test/* && jest -w 4 build/test-slow/*",
        "clean": "rm -rf build",
        "tslint": "tslint -p tsconfig.json"
    },
    "author": "Andy Chou",
    "license": "ISC",
    "dependencies": {
        "@types/semver": "^5.5.0",
        "archiver": "^3.0.0",
        "aws-sdk": "^2.382.0",
        "axios": "^0.18.0",
        "commander": "^2.15.1",
        "debug": "^4.1.1",
        "googleapis": "^36.0.0",
        "inquirer": "^6.2.1",
        "jszip": "^3.1.5",
        "listr": "^0.14.3",
        "memory-fs": "^0.4.1",
        "ora": "^3.0.0",
        "semver": "^5.6.0",
        "tslib": "^1.9.1",
        "uuid": "^3.2.1",
        "webpack": "^4.28.2",
        "webpack-node-externals": "^1.7.2",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "@types/archiver": "^2.1.1",
        "@types/aws-lambda": "^8.10.17",
        "@types/debug": "^0.0.31",
        "@types/express": "^4.11.1",
        "@types/inquirer": "^0.0.43",
        "@types/jest": "^23.3.10",
        "@types/jszip": "^3.1.4",
        "@types/listr": "^0.13.0",
        "@types/loader-utils": "^1.1.3",
        "@types/memory-fs": "^0.3.0",
        "@types/node": "^10.12.18",
        "@types/ora": "^1.3.4",
        "@types/papaparse": "^4.5.5",
        "@types/tar-stream": "^1.6.0",
        "@types/uuid": "^3.4.3",
        "@types/webpack": "^4.4.22",
        "@types/webpack-node-externals": "^1.6.3",
        "@types/yauzl": "^2.9.0",
        "jest": "^23.1.0",
        "papaparse": "^4.6.2",
        "tslint": "^5.12.0",
        "tslint-config-prettier": "^1.17.0",
        "typescript": "^3.2.2",
        "webpack-cli": "^3.1.0"
    },
    "jest": {
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json",
            "node"
        ],
        "roots": [
            "build/test",
            "build/test-slow"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules"
        ],
        "testEnvironment": "node"
    }
}
