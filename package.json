{
    "name": "faastjs",
    "version": "0.0.2",
    "description": "Serverless without the cruft.",
    "main": "build/index.js",
    "types": "build/decls/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/faastjs/faast.js"
    },
    "scripts": {
        "build": "npm run clean && npx tsc && npm run doc",
        "watch": "npm run clean && npx tsc --watch",
        "doc": "node build/src/doc.js",
        "set-gcp-key": "node build/src/google/google-save-key.js",
        "test": "ava build/test/*.test.js",
        "test-local": "ava --concurrency=4 -m='!remote*'",
        "test-aws": "ava --concurrency=8 --timeout=2m -m='!remote*' -m='*aws*'",
        "test-google": "ava --concurrency=4 --timeout=4m -m='!remote*' -m='*google*'",
        "test-aws-remote": "ava --concurrency=8 --timeout=2m -m='remote*aws*'",
        "test-google-remote": "ava --concurrency=4 --timeout=4m -m='remote*google*'",
        "test-debug": "DEBUG=faast:provider,faast:info ava --fail-fast --serial",
        "cleanup-cloud": "node build/src/cli.js cleanup aws -x -f && node ./build/src/cli.js cleanup google -x -f",
        "clean": "rm -rf build",
        "tslint": "npx tslint -p tsconfig.json"
    },
    "author": "Andy Chou",
    "license": "Apache-2.0",
    "bin": {
        "faastjs": "./build/src/cli.js"
    },
    "dependencies": {
        "archiver": "^3.0.0",
        "aws-sdk": "^2.423.0",
        "commander": "^2.15.1",
        "debug": "^4.1.1",
        "fs-extra": "^7.0.1",
        "googleapis": "^38.0.0",
        "listr": "^0.14.3",
        "memory-fs": "^0.4.1",
        "ora": "^3.2.0",
        "process-doctor": "^1.0.0",
        "tslib": "^1.9.1",
        "uuid": "^3.2.1",
        "webpack": "^4.29.6",
        "webpack-node-externals": "^1.7.2",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "@microsoft/api-documenter": "^7.0.35",
        "@microsoft/api-extractor": "^7.0.28",
        "@types/archiver": "^2.1.1",
        "@types/aws-lambda": "^8.10.23",
        "@types/debug": "^4.1.2",
        "@types/express": "^4.16.1",
        "@types/listr": "^0.13.0",
        "@types/loader-utils": "^1.1.3",
        "@types/lolex": "^3.1.0",
        "@types/memory-fs": "^0.3.0",
        "@types/node": "^11.11.3",
        "@types/papaparse": "^4.5.9",
        "@types/tar-stream": "^1.6.0",
        "@types/uuid": "^3.4.3",
        "@types/webpack": "^4.4.25",
        "@types/webpack-node-externals": "^1.6.3",
        "@types/yauzl": "^2.9.1",
        "ava": "^1.3.1",
        "lolex": "^3.1.0",
        "papaparse": "^4.6.3",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.3.3333",
        "webpack-cli": "^3.3.0"
    },
    "ava": {
        "files": [
            "build/test/*.test.js"
        ]
    },
    "prettier": {
        "tabWidth": 4
    }
}
